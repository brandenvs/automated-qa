<!DOCTYPE html>
<html lang="en" data-bs-theme="dark">
<!--HEAD-RESOURCES-->
<head>
    <meta charset="utf-8">
    <!-- View-Meta-Data-^^Encoding -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- View-Title -->
    <title>@ViewData["Title"] - AWAIT USER</title>
    <!--(ROOT-CSS)Custom-StyleSheet-->
    <link rel="stylesheet" href="~/css/css-users/site-await.custom.css" asp-append-version="true" />
    <!--(ROOT-CSS)Site-StyleSheet-->
    <link rel="stylesheet" href="~/css/css-custom/styles.css" asp-append-version="true" />
    <!--(GCP)Web-Font-->
    <link href="https://fonts.googleapis.com/css?family=Lato:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&amp;display=swap" rel="stylesheet">
    <!--(CDN)Bootstrap-Icons-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css" integrity="sha384-b6lVK+yci+bfDmaY1u0zE8YYJt0TZxLEAFyYSLHId4xoVvsrQu3INevFKo+Xir8e" crossorigin="anonymous">
    <!--(CDN)Bootstrap-v5.3.0-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <!--Razor-Engine-Renders-Styles->[FALSE]-->
    @RenderSection("Styles", required: false)
</head>
<!--BODY-->
<body class="p-3 m-0 border-0">
    <!--HEADER--->
    <header>
        @ViewData["Title"] Responded!<br />Nothing to show... :(
    </header>
    <!--CONTENT-->
    <div class="container-fluid p-2 mb-3">
        <main role="main" class="pb-3">
            <!--Razor-Engine-Renders-View->[TRUE]-->
            @RenderBody()
        </main>
    </div>
    <!--FOOTER-->
    <footer class="bg-dark text-body-secondary">
        <!--Footer-Container-->
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-md-6 col-12">
                    <div class="d-flex flex-column" style="height:100%;">
                        <div class="row p-2 mb-3 border border-secondary-subtle">
                            <div class="col-4">
                                <img src="/media/logo-removebg.png" class="img-fluid" alt="logo-footer">
                            </div>
                            <div class="col-8">
                                <figure class="p-2">
                                    <blockquote class="blockquote text-light-emphasis">
                                        <p><i class="bi bi-asterisk"></i> Bespoke Automated Testing Solutions</p>
                                    </blockquote>
                                    <figcaption class="blockquote-footer text-secondary-emphasis">
                                        <p class="lead text-secondary-emphasis">
                                            Comprehensive, Adaptable & Rapid Custom Automated Testing Solutions Designed Around Your IT Infrastructure!
                                        </p>
                                        <h5 class="text-secondary-emphasis"><i class="bi bi-telephone-outbound-fill text-light-emphasis"></i> CONTACT US</h5>
                                        Email - <cite title="Source Title"><a href="#" class="link-info link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover">contactawait@gmail.com</a></cite><br>
                                        Office Phone - <cite title="Source Title"><a href="#" class="link-info link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover">+27 08 565 0206</a></cite>
                                    </figcaption>
                                </figure>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-12">
                    <div class="d-flex flex-column" style="height:100%;">
                        <figure class="text-center p-2">
                            <blockquote class="blockquote">
                                <p class="h4 text-light-emphasis">About</p>
                            </blockquote>
                            <figcaption class="blockquote-footer text-center">
                                <p class="lead text-secondary-emphasis">
                                    Our expertise lies in streamlining QA processes for websites, web applications, and complex systems. With a focus on data privacy and security, we ensure the confidentiality of your information. Partner with us for efficient and reliable testing, driven by cutting-edge technologies and industry best practices. Contact us today to propel your IT infrastructure to new heights.
                                </p>
                                <div class="text-end">
                                    <u>Disclaimer</u> - <cite title="Source Title">This is a prototype, most functionalities are still being developed.</cite>
                                </div>
                            </figcaption>
                        </figure>
                    </div>
                </div>
                <hr>
                <div class="col-md-6 col-6 text-start">
                    &copy; 2023 AWAIT. All rights reserved.
                </div>
                <div class="col-md-6 col-6 text-end">
                    Your Privacy Matters - <a href="Home/Privacy" class="text-reset link-light link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover">Privacy Policy</a>
                </div>
            </div>
        </div>
    </footer>
    <!--SCRIPTS-CDN-LINKS-->
    <!--(CDN)Bootstrap-v5.3.0-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
    <!--(API)JQuery-v3.5.1-->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!--(LIB)JQuery-v3.5.1-->
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <!--(LIB)Bootstrap-v5.0.2-->
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <!--(ROOT-JS)Custom-JavaScript-->
    <script src="~/js/js.users/site.js" asp-append-version="true"></script>
    <!--Razor-Engine-Renders-Styles->[FALSE]-->
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
